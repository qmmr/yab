{
  "name": "YAB",
  "version": "1.0.0",
  "description": "Yet Another Boilerplate - ES2015, react, webpack, karma, mocha, chai, enzyme, css-modules",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "build": "webpack --config webpack.config.production.js",
    "test": "karma start --single-run --no-auto-watch karma.config.js",
    "test:watch": "karma start --auto-watch --no-single-run karma.config.js",
    "lint": "eslint ./src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qmmr/yab.git"
  },
  "keywords": [
    "react",
    "es2015",
    "babel",
    "webpack",
    "karma",
    "mocha",
    "chai",
    "enzyme",
    "css-modules"
  ],
  "author": "Marcin Kumorek (http://qmmr.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qmmr/yab/issues"
  },
  "homepage": "https://github.com/qmmr/yab",
  "dependencies": {
    "babel-runtime": "^6.9.2",
    "react": "0.14.7",
    "react-dom": "0.14.7"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-2": "6.13.0",
    "chai": "3.5.0",
    "css-loader": "0.23.1",
    "enzyme": "2.4.1",
    "eslint": "3.3.1",
    "eslint-config-qmmr": "2.0.7",
    "eslint-import-resolver-node": "^0.2.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "2.1.0",
    "eslint-plugin-react": "6.1.1",
    "istanbul-instrumenter-loader": "^0.2.0",
    "json-loader": "0.5.4",
    "karma": "1.2.0",
    "karma-chai": "0.1.0",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "1.1.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack-with-fast-source-maps": "1.9.2",
